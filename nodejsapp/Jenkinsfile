pipeline{
    agent none
    stages{
        stage('build'){
            agent {
                dockerfile{
                    environement{
                      imageName = 'adam/nodejs'
                    }
                 label 'build-slave'
                 dir 'nodejsapp'
                }
            }
            steps{
                sh 'node --version'
            } 
         }
    }
}
